---
permalink: /feed.xml
---
<?xml version="1.0" encoding="UTF-8" ?>
<rss version="2.0">
  <channel>
    <title>{{ metadata.title }}</title>
    <link>{{ metadata.url }}</link>
    <description>{{ metadata.description }}</description>
	  {%- for sheet in collections.sheet -%}
	  {%- if sheet.data.publishDate -%}
    <item>
      <title>{{ sheet.data.displayTitle }}</title>
      <link>{{ metadata.url + sheet.url }}</link>
      <pubDate>{{ sheet.data.publishDate | rssDate }}</pubDate>
      <description>{{ sheet.data.description }} is out now!</description>
    </item>
    {%- endif -%}
    {%- endfor -%}
  </channel>
</rss>